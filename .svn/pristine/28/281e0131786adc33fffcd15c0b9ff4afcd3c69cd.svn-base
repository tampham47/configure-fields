        $(document).ready(function (e) {
            $("#t20").click(function (e) {
                $(".main-content").css('display', 'block');
                $(".main-content2").css('display', 'none');
                $("#all-footer").css('display', 'block');
                $('.main-content-header').css('display', 'none');
            });
            $("#t20c").click(function (e) {
                $(".main-content1").css('display', 'block');
                $(".main-content22").css('display', 'none');
                $("#all-footer").css('display', 'block');
                $('.main-content-header').css('display', 'none');
            });
            $("#t21").click(function (e) {
                $(".main-content2").css('display', 'block');
                $(".main-content").css('display', 'none');
                $("#all-footer").css('display', 'block');
                $('.main-content-header').css('display', 'none');
            });
            $("#t21c").click(function (e) {
                $(".main-content22").css('display', 'block');
                $(".main-content1").css('display', 'none');
                $("#all-footer").css('display', 'block');
                $('.main-content-header').css('display', 'none');
            });
            $('.videolink').click(function (e) {
                $('.main-content2').css('display', 'none');
                $('.main-content22').css('display', 'none');
                $('.main-content1').css('display', 'none');
                $('.main-content').css('display', 'block');
                $('.main-content-header').css('display', 'block');
                $("#footer-area").css('display', 'none');
                $("#all-footer").css('display', 'none');
            });
            $(".foter-finish").click(function (e) {
                $("#footer-area").css('display', 'none');
            });


            $('#click').on('click', function () {
                $('.drop-down').slideToggle('fast');
            });
            $('#t21').click(function (e) {
                $('#t21').css('border-bottom', '1px solid #000');
                $('#t20').css('border', 'none');
            });
            $('#t20').click(function (e) {
                $('#t20').css('border-bottom', '1px solid #000');
                $('#t21').css('border', 'none');
            });


            $('#t21c').click(function (e) {
                $('#t21c').css('border-bottom', '1px solid #000');
                $('#t20c').css('border', 'none');
            });
            $('#t20c').click(function (e) {
                $('#t20c').css('border-bottom', '1px solid #000');
                $('#t21c').css('border', 'none');
            });

        });

        $(window).bind('scroll', function () {
            if ($(window).scrollTop() > 70) {
                $('.header').css('display', 'none');
                $('.header-after').css('display', 'table');
                $('#footer-area').addClass('footer-area-fixed');
                $('.main-content').addClass('main-content1');
                $('.main-content2').addClass('main-content22');
            }
            else {
                $('.header').css('display', 'table');
                $('.header-after').css('display', 'none');
                $('#footer-area').removeClass('footer-area-fixed');
                $('.main-content').removeClass('main-content1');
                $('.main-content2').removeClass('main-content22');
            }
        });
